{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books. With this application users are able to find and rent books. The application also has an admin section where the admin can do things like add books, delete books, increase the quantity of a book etc.",
  "main": "./server/index.js",
  "engines": {
    "node": "6.10.3"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "client:build": "webpack --config ./client/webpack.prod.js --progress --colors",
    "server:build": "babel -d ./dist ./server -s",
    "start:server": "nodemon --exec babel-node server/index.js",
    "scss": "./node_modules/.bin/node-sass --watch ./templates/scss/style.scss -o ./templates/css",
    "start:client": "node ./client/devServer.js",
    "heroku-postbuild": "npm run client:build && npm run server:build",
    "serve": "node ./dist/index.js",
    "test:client": "jest -u && codecov",
    "test": "export NODE_ENV=test && sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all && babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive -R spec ./server/tests --timeout 10000 --compilers:js babel-core/register",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "db:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migration": "sequelize db:migrate",
    "all:tests": "npm test && npm run test:client",
    "seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amarachukwu-agbo/hello-books.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amarachukwu-agbo/hello-books/issues"
  },
  "jest": {
    "rootDir": "client",
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__",
      "<rootDir>/coverage",
      "<rootDir>/src",
      "<rootDir>/src/store.js",
      "<rootDir>/devServer.js",
      "<rootDir>/webpack.common.js",
      "<rootDir>/webpack.dev.js",
      "<rootDir>/webpack.prod.js",
      "<rootDir>/reducers/index.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTests.js",
    "testMatch": [
      "**/**/__tests__/**/*.spec.js?(x)",
      "**/**/__tests__/?(*.)(spec|test).js?(x)"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js?(x)"
    ]
  },
  "homepage": "https://github.com/amarachukwu-agbo/hello-books#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.18",
    "codecov": "^3.0.3",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^1.0.0-alpha",
    "jest": "22.4.3",
    "jsdoc": "^3.5.5",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.17.4",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.7",
    "sequelize-cli": "^3.0.0",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.7",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "identity-obj-proxy": "^3.0.0",
    "joi": "^13.0.2",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.1.0",
    "materialize-css": "^0.100.2",
    "moment": "^2.22.0",
    "node-sass": "^4.8.3",
    "nodemailer": "^4.6.7",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-dropzone": "^4.2.11",
    "react-hot-loader": "^4.0.0",
    "react-materialize": "^2.2.1",
    "react-modal": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-widgets": "^4.2.6",
    "react-widgets-moment": "^4.0.12",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "save-dev": "^2.0.0",
    "sequelize": "^4.37.4",
    "sinon": "^4.5.0",
    "superagent": "^3.8.3",
    "superagent-nock": "^0.4.0",
    "supertest": "^3.0.0",
    "swagger-ui-express": "^3.0.10",
    "sweetalert": "^2.1.0",
    "url-loader": "^1.0.1",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2"
  }
}
