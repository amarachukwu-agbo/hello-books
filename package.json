{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books. With this application users are able to find and rent books. The application also has an admin section where the admin can do things like add books, delete books, increase the quantity of a book etc.",
  "main": "./server/index.js",
  "engines": {
    "node": "6.10.3"
  },
  "scripts": {
    "scss": "./node_modules/.bin/node-sass --watch ./templates/scss/style.scss -o ./templates/css",
    "build": "webpack --config ./client/webpack.prod.js",
    "start": "./node_modules/.bin/babel-node ./server/index.js",
    "start:dev": "node ./client/devServer.js",
    "heroku-postbuild": "npm install --production",
    "serve": "node ./dist/index.js",
    "test": "set NODE_ENV=test && sequelize db:migrate && babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive -R spec ./server/Test/api-postgres-test.js --timeout 10000 --compilers:js babel-core/register",
    "tests": " set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate&&babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive -R spec ./server/Test/api-postgres-test.js --timeout 10000 --compilers:js babel-core/register",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amarachukwu-agbo/hello-books.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amarachukwu-agbo/hello-books/issues"
  },
  "homepage": "https://github.com/amarachukwu-agbo/hello-books#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.18",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "file-loader": "^1.1.11",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.7",
    "sequelize-cli": "^3.0.0",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
